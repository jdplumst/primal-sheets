{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "bunVersion": "1.x",
  "buildCommand": "tsc && vite build",
  "outputDirectory": "dist",
  "installCommand": "bun install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-worker.workers.dev/api/:path*"
    }
  ]
}
